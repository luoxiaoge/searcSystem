<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.roymark.mapper.SpecialistMapper">
 <select id="findSpecial" resultType="Specialist">
   select * from specialist;
</select>
<insert id="insertSpecialist" >
    insert into specialist(s_name,s_photo,s_introduction) values(#{0},#{1},#{2}) 
</insert>
</mapper>
